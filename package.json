{
  "name": "@trigo/project-template",
  "private": true,
  "version": "0.0.0",
  "description": "An opinionated project template by TRIGO",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "make:template": "npx tsc -p ./deploy/generate && npx tcoa ./deploy/generate/generate.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trigo-at/project-template.git"
  },
  "keywords": [
    "template",
    "TRIGO"
  ],
  "author": "TRIGO GmbH <hello@trigo.at>",
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.3",
    "@trigo/trigo-create-openshift-app": "^0.0.7",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-trigo": "^18.0.5",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "hygen": "^6.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
